<%= form_with model: [:admin, @test] do |form| %>
  <%= render 'shared/errors', resource: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title, id: :test_title %>
  </p><br>

  <p>
    <%= form.label :level %><br>
    <%= form.number_field :level %>
  </p><br>

  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <%= hidden_field_tag :completed, '' %>
  <%= check_box_tag :completed, checked = true %>

  <p><%= form.submit %></p>
<% end %>